---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Sandro Padin</title>
 <link href="http://sa.ndropad.in/atom.xml" rel="self"/>
 <link href="http://sa.ndropad.in/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://sa.ndropad.in/</id>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <category term="{{post.categories}}" />
   <link href="http://sa.ndropad.in{{ post.url }}"/>
   <author>
     <name>{{ post.categories | replace:'-',' ' }}</name>
   </author>
   <updated>{{ post.date | date_to_string }}</updated>
   <id>http://sa.ndropad.in{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
